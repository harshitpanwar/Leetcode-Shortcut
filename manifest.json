{
    "manifest_version": 3,
    "name": "LeetCode Submitter",
    "description": "Base Level Extension",
    "version": "1.0",
    "action": {
      "default_popup": "leetcode.html",
      "default_icon": "leetcode.png"
    },
    "content_scripts": [
        {
          "js": ["scripts/background.js"],
          "matches": [
            "https://leetcode.com/*"
          ]
        }   
    ]
  }